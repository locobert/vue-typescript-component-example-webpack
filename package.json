{
  "name": "vue-typescript-component-example-webpack",
  "version": "1.0.0",
  "description": "Experimental example for using vue-typescript-component with webpack",
  "author": "Locobert",
  "private": true,
  "scripts": {
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "test": "jest",
    "lint": "tslint src/**/*.ts test/**/*.ts"
  },
  "jest": {
    "transform": {
      ".*\\.(ts|vue)$": "<rootDir>/node_modules/vue-typescript-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "vue"
    ],
    "testRegex": "/test/.*\\.(ts|js)$",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/.*\\.(ts|js)$",
      "/.*\\.vue$"
    ]
  },
  "dependencies": {
    "vue": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^16.0.0",
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.0.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.1.0",
    "css-loader": "^0.25.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "function-bind": "^1.0.2",
    "html-webpack-plugin": "^2.8.1",
    "http-proxy-middleware": "^0.17.2",
    "jest-cli": "^17.0.0",
    "json-loader": "^0.5.4",
    "opn": "^4.0.2",
    "ora": "^0.3.0",
    "pug": "^2.0.0-beta6",
    "semver": "^5.3.0",
    "shelljs": "^0.7.4",
    "ts-loader": "^1.2.2",
    "tslint": "^4.0.1",
    "tslint-config-locoslab": "^1.0.0",
    "typescript": "^2.0.10",
    "url-loader": "^0.5.7",
    "vue-jest-utils": "^0.2.0",
    "vue-loader": "^10.0.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.0",
    "vue-typescript-component": "^0.6.1",
    "vue-typescript-import-dts": "^2.0.0",
    "vue-typescript-jest": "^0.3.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
